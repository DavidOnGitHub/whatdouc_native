{
  "plugins": ["import", "react", "react-native", "prettier", "class-property"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "always"],
    "prettier/prettier": ["error", { "singleQuote": true }],
    "react/prop-types": "off"
  }
}
